<%= form_with(model: @user, url: users_create_path, local: true) do |f| %>
    <div>
        <%= f.text_field :email, placeholder: 'Ingresa tu email' %>
    </div>
    <div>
        <%= f.password_field :password, placeholder: 'Ingresa tu contraseña' %>
    </div>
    <div>
        <%= f.password_field :password_confirmation, placeholder: 'Ingresa tu contraseña' %>
    </div>
    <%= f.submit "Registrarse" %>
<% end %>